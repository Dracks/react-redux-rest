{
  "moduleFileExtensions": [
    "js",
    "ts",
    "tsx"
  ],
  "globals": {
    "ts-jest": {
      "tsConfig": "tsconfig.json"
    }
  },
  "testMatch": [
    "**/*.test.(ts|tsx|js)"
  ],
  "testPathIgnorePatterns":[
    "/node_modules/",
    "/lib/"
  ],
  "preset": "ts-jest",
  "setupFiles": ["<rootDir>/.jestSetup.js"]
}